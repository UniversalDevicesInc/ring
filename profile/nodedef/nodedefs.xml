<nodeDefs>

  <nodeDef id="CTL" nodeType="139" nls="CTL">
    <sts>
      <st id="ST" editor="boolean" /> <!-- Online -->
	</sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="DISCOVER" />
        <cmd id="QUERYALL"/>
      </accepts>
    </cmds>
  </nodeDef>

  <!-- Doorbell Main node -->
  <nodedef id="DOORBELL" nls="DB">
    <sts>
      <st id="ST" editor="boolean"/> <!-- Online -->
      <st id="BATLVL" editor="percent"/> <!-- Battery level -->
      <st id="GV0" editor="percent"/> <!-- Battery level 2 -->
      <st id="GV1" editor="mv"/> <!-- Battery level mV -->
    </sts>
    <cmds>
      <sends>
        <cmd id="DON"/>
      </sends>
      <accepts>
        <cmd id="QUERY"/>
      </accepts>
    </cmds>
  </nodedef>

  <!-- Doorbell Motion Node -->
  <nodedef id="DOORBELLMOTION" nls="DBM">
    <sts/>
    <cmds>
      <sends>
        <cmd id="DON"/>
      </sends>
      <accepts>
      </accepts>
    </cmds>
  </nodedef>

  <!-- Camera Motion Node -->
  <nodedef id="CAMERA" nls="CAM">
    <sts>
      <st id="ST" editor="boolean"/> <!-- Online -->
      <st id="BATLVL" editor="percent"/> <!-- Battery level -->
      <st id="GV0" editor="percent"/> <!-- Battery level 2 -->
      <st id="GV1" editor="mv"/> <!-- Battery level mV -->
    </sts>
    <cmds>
      <sends>
        <cmd id="DON"/>
      </sends>
      <accepts>
        <cmd id="QUERY"/>
      </accepts>
    </cmds>
  </nodedef>

  <!-- Floodlight - This is an additional node for cameras which has lights -->
  <nodedef id="LIGHT" nls="L">
    <sts>
      <!-- <st id="ST" editor="_78_0"/> Floodlight state not available in the API -->
    </sts>
    <cmds>
      <sends>
        <cmd id="DON"/>
        <cmd id="DOF"/>
      </sends>
      <accepts>
        <cmd id="DON"/>
        <cmd id="DOF"/>
      </accepts>
    </cmds>
  </nodedef>

</nodeDefs>
