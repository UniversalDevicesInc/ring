<nodeDefs>

  <nodeDef id="CTL" nodeType="139" nls="CTL">
    <sts>
      <st id="ST" editor="boolean" />
	</sts>
    <cmds>
      <sends />
      <accepts>
        <cmd id="DISCOVER" />
      </accepts>
    </cmds>
  </nodeDef>

  <!-- Doorbell Main node -->
  <nodedef id="DOORBELL" nls="DB">
    <sts>
      <st id="ST" editor="boolean"/> <!-- Online -->
      <st id="BAT" editor="percent"/> <!-- Battery level -->
      <st id="BAT2" editor="percent"/> <!-- Battery level 2 -->
      <st id="BATMV" editor="mv"/> <!-- Battery level mV -->
      <st id="ERR" editor="boolean" hide="T" />
    </sts>
    <cmds>
      <sends>
        <cmd id="DON"/>
      </sends>
      <accepts>
        <cmd id="DON"/>
        <cmd id="QUERY"/>
      </accepts>
    </cmds>
  </nodedef>

  <!-- Doorbell Motion Node -->
  <nodedef id="DOORBELLMOTION" nls="DBM">
    <sts/>
    <cmds>
      <sends>
        <cmd id="DON"/>
        <cmd id="DOF"/>
      </sends>
      <accepts>
        <cmd id="DON"/>
      </accepts>
    </cmds>
  </nodedef>

  <!-- Camera Motion Node -->
  <nodedef id="CAMERA" nls="CAM">
    <sts>
      <st id="ST" editor="boolean"/> <!-- Online -->
      <st id="BAT" editor="percent"/> <!-- Battery level -->
      <st id="BAT2" editor="percent"/> <!-- Battery level 2 -->
      <st id="BATMV" editor="mv"/> <!-- Battery level mV -->
      <st id="ERR" editor="boolean" hide="T" />
    </sts>
    <cmds>
      <sends>
        <cmd id="DON"/>
        <cmd id="DOF"/>
      </sends>
      <accepts>
        <cmd id="DON"/>
        <cmd id="QUERY"/>
      </accepts>
    </cmds>
  </nodedef>

  <!-- Floodlight - This is an additional node for cameras which has lights -->
  <nodedef id="FLOOD" nls="FL">
    <sts>
      <!-- <st id="ST" editor="_78_0"/> Floodlight state not available in the API -->
    </sts>
    <cmds>
      <sends>
        <cmd id="DON"/>
        <cmd id="DOF"/>
      </sends>
      <accepts>
        <cmd id="DON"/>
        <cmd id="DOF"/>
      </accepts>
    </cmds>
  </nodedef>

